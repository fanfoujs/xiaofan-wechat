<import src="/components/templates.wxml" />
<block wx:if="{{feed}}">
  <view class="aboveToolbar" style="padding-top:{{param ? 222: 0}}px">
    <template is="feeds_context" data="{{feed, feeds_arr}}" />
  </view>
  <template is="toolbar" data="{{feed: feed}}" />
</block>
<block wx:if="{{param}}">
  <view class="mask">
    <template is="post" data="{{value: param.status, photoPaths, isRepost: param.isRepost}}" />
  </view>
</block>
